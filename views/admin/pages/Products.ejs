                    </div>
                </div>
            </div>
            <form action="" method="post" >
              <div class="col-lg-12">
                <div class="card mb-4">
                    <div class="card-body">
                        <div class="mb-4">
                            <label for="product_title" class="form-label" style="font-size:25px;">Product List </label>
                            <div>                             
                              <a href="/admin/addProducts" class="btn btn-primary btn-sm rounded">Create new</a>
                          </div>
                            <br>
                            <br>
                            <table style="width: 100%;" id="example" class="table table-striped table-bordered table-sm" cellspacing="0"
                            width="100%">
                            <thead>
                                <tr>
                                    <th class="th-sm">Image</th>
                                    <th class="th-sm">Product Name</th>
                                    <th class="th-sm">Collections</th>
                                    <th class="th-sm">Category</th>
                                    <th class="th-sm">SKU</th>
                                    <th class="th-sm">Sale Price</th>
                                    <th>List/Unlist</th>
                                    <th class="th-sm">Actions</th>
                                </tr>
                            </thead>
                            <tbody>
            
            
                                <% productList.forEach(element=> { %>
                                    <tr>
                                        <td><img src="<%= element?.images[0]?.thumbnailUrl %>"
                                                style="max-width: 75px;max-height: 75px;" alt="Product Image"></td>
                                        <td>
                                            <%= element.productName %>
                                        </td>
                                        <td>
                                            <%= element.collectionName?.collectionName %>
                                        </td>
                                        <td>
                                            <%= element.categoryName?.categoryName %>
                                        </td>
                                        <td>
                                            <%= element.quantity %>
                                        </td>
                                        <td>
                                            <%= element.salePrice %>
                                        </td>
                                        <td>
                                            <% if (element.isListed) { %>
            
                                                <form action="/admin/pages/Products/UnList/<%- element?._id %>" method="post">
                                                    <button type="submit"
                                                        class="btn btn-danger btn-sm unblock-button">Unlist</button>
                                                </form> 
            
                                                <% } else { %>
                                                    <form action="/admin/Products/list/<%- element?._id %>" method="post">
                                                        <button type="submit"
                                                            class="btn btn-success btn-sm block-button">List</button>
                                                    </form>
                                                    <% } %>
                                        </td>
                                        <td>
                                            <a href="/admin/Products/editProduct/<%- element._id %>">
                                                <button class="btn btn-info btn-sm ml-2 "><i
                                                        class="material-icons md-edit pb-1"></i></button>
                                            </a>
                                        </td>
                                    </tr>
                                    <% }); %>
                            </tbody>
                        </table>
                    </div>
            
                </section>
                <script>
                    $(document).ready(function () {
                        new DataTable('#example');
            
                    })
                </script>
